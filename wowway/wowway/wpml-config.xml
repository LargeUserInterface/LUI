<wpml-config>
    <custom-fields>
        <custom-field action="ignore">pp_gallery_slider</custom-field>
        <custom-field action="ignore">krown_project_m_width</custom-field>
        <custom-field action="ignore">krown_project_m_height</custom-field>
        <custom-field action="ignore">krown_project_m_slider_width</custom-field>
        <custom-field action="ignore">rb_post_pass</custom-field>
